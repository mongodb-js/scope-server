{
  "name": "scout-server",
  "description": "Uses the nodejs native driver to provide an http api to mongodb",
  "version": "0.0.4",
  "scripts": {
    "ci": "gulp ci",
    "test": "mocha",
    "start": "nodemon --no-stdin --watch lib bin/scout-server.js",
    "check": "mongodb-js-precommit",
    "security-audit": "nsp package"
  },
  "precommit": [
    "check",
    "security-audit"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/10gen/scout.git"
  },
  "bin": {
    "scout-server": "bin/scout-server.js"
  },
  "dependency-check": {
    "entries": [
      "index.js"
    ],
    "ignore": [
      "mongodb"
    ]
  },
  "dependencies": {
    "async": "^1.3.0",
    "body-parser": "^1.13.1",
    "boom": "^2.8.0",
    "csv-write-stream": "^1.0.0",
    "debug": "^2.2.0",
    "event-source-emitter": "0.0.2",
    "event-stream": "^3.3.1",
    "express": "^4.13.0",
    "joi": "^6.5.0",
    "lodash": "^3.10.0",
    "mongodb": "^2.0.35",
    "mongodb-collection-sample": "^0.1.0",
    "mongodb-extended-json": "^1.3.1",
    "mongodb-favicon": "^0.0.1",
    "mongoscope-config": "^0.1.0",
    "peek-stream": "^1.1.1",
    "scout-brain": "http://bin.mongodb.org/js/scout-brain/v0.0.2/scout-brain-0.0.2.tar.gz",
    "socket.io": "^1.3.5",
    "socket.io-stream": "0.6.1",
    "socketio-jwt": "^4.2.0"
  },
  "devDependencies": {
    "coffee-script": "^1.9.3",
    "eslint-config-mongodb-js": "^0.1.4",
    "mocha": "^2.2.5",
    "mongodb-js-precommit": "^0.1.2",
    "mongodb-runner": "^2.2.0",
    "mongodb-version-manager": "^0.7.1",
    "nodemon": "^1.3.7",
    "nsp": "^1.0.3",
    "pre-commit": "^1.0.10",
    "supertest": "^1.0.1"
  }
}
